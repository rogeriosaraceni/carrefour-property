---
import Head from '@includes/Head.astro';
import Container from "@layouts/Container.astro";
import AppCards from './AppCards.astro';

const response = await fetch(
    "https://official-joke-api.appspot.com/random_ten"
)
const data = await response.json()

const data2 = [
    {
        "id": 1,
        "setup": "Setup 1"
    },
    {
        "id": 2,
        "setup": "Setup 2"
    }
]
---

<!DOCTYPE html>
<html lang="pt-br">
    <Head />

    <body>
        <main class="main-app">
            <Container>
                <ul class="list-group list-unstyled">
                    {
                        data2.map((item) => {
                            return (
                                <AppCards
                                url='#'
                                id={item.id}
                                setup={item.setup} />
                            )
                        })
                    }
                </ul>
            </Container>
        </main>
    </body>
</html>
